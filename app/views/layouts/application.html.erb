<!DOCTYPE html>
<html>
<head>
  <title>Materialize Photo Gallery</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<body>

  <% flash.each do |key, value| %>
    <div class="alert alert<%= key %>">
      <ul>
        <li>
        <%= value %>
      </li>
      </ul>
    </div>
  <% end %>

<%= yield %>

</body>
</html>
